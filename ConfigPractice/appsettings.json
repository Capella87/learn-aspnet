{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MapSettings": {
    "DefaultZoomLevel": 9,
    "DefaultLocation": {
      "latitude": 50.500,
      "longitude": -4.000
    }
  },
  "AppDisplaySettings": {
    "Title": "ACME Store Locator",
    "ShowCopyright": true
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2AndHttp3",
      "ClientCertificateMode": "AllowCertificate",
      "SslProtocols": [ "Tls12", "Tls13" ]
    },
    "Endpoints": {
      "HttpsDefaultCert": {
        "Url": "https://localhost:20621",
        "Protocols": "Http1AndHttp2AndHttp3"
      }
    }
  }
}